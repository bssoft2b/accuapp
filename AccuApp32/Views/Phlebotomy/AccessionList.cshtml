            <th>PatientName</th>
            <th>PatientDOB</th>
            <th>PatientGender</th>
            <th>InsuranceName</th>
            <th>InsuranceID</th>
            <th>Note</th>
            <th>Specimen Was Sent To Other Lab</th>
            <th>Specimen Was Not Sent To Other Lab</th>
            <th>Specimen Can Be Sent To Accu</th>
            <th>Specimen Cannot Be Sent To Accu</th>
            <th>Last Modified By</th>
            <th>Created By</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tfoot>
