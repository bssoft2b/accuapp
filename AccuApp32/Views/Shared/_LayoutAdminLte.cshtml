            $(function(){

                let menuOpt=localStorage.getItem("menuOpt");

                if(menuOpt!=null){

                    let mOpt="a[menu-opt="+menuOpt+"]";

                    $(mOpt).css("background-color","white");
                    $(mOpt).css("color","black");
                    $(mOpt).trigger("click");
                    $(mOpt).closest("li.has-treeview").addClass("menu-open");
                }

                $("a.option").click(function(){
                    let menuOpt=$(this).attr("menu-opt");
                    localStorage.setItem("menuOpt",menuOpt);
